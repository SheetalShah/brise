<% if object.has_any_treeView_objects_for([object]).any? %>
  <ul>
    <% objects = object.has_any_treeView_objects_for([object]) %>
    <% object.treeView_next_level %>
    <% objects.each do |currentnode| %>
      <%= render 'shared/objectnode', :object => object, :currentnode => currentnode, :level => "level1" %>
    <% end %>
  </ul>
<% end %>

