<!DOCTYPE html5> 
<html lang="en"> 
<head> 
<%= javascript_include_tag "jquery-ui.min.js", "highcharts", "exporting" %>
<%= stylesheet_link_tag "gmaps4rails" %>

<meta charset="utf-8" name="viewport" content="width=device-width, initial-scale=1.0"> 
</head>
<body>
  <%= render 'shared/header' %>
   <div class="container-fluid">
    <div class="row-fluid">
     <aside>
        <% if params[:ad_id] || params[:product_id] || params[:user_id] %>
        <div class="span2">
        </div>
        <div class="span7">  
<% if @ad %>
            <section>
              <%= render 'shared/adInfo' %>
            </section>
          <% elsif @product %>
            <section>
              <%= render 'shared/objectInfo' %>
            </section>
          <% else %>
  	    <section>
 	      <%= render 'shared/userInfo' %>
  	    </section>
          <% end %>
       <% else %>
       <div class="span12">
       <% end %>
          <% if flash[:notice] %>
  	  <redcolor>*<%= flash[:notice] %>*</redcolor>
          <% end %>
          <%= render 'shared/adfeed' %>
        </div>
      </aside>
    </div>
</div>
  </body>
  <footer>
	<%= debug(params) %>
	<%= yield :scripts %> 
  </footer>
</html>
